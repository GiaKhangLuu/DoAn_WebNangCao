@model List<DoAn_WebNangCao.Models.DETHI>

@{
    ViewBag.Title = "TestLog";
    Layout = "~/Views/Shared/_LayoutClient.cshtml";
}


<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Lịch sử Thi</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Lĩnh Vực</th>
                        <th>Họ Tên</th>
                        <th>Ngày Thi</th>
                        <th>Điểm</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.DANHSACHCAUHOIs.Where(x => x.IDDeThi == item.IDDeThi).FirstOrDefault().CAUHOI.LINHVUC.TenLinhVuc)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TAIKHOAN.HoTen)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.NgayThi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TongDiem)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
<!-- /.container-fluid -->
